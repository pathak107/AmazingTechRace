<%- include('./adminPartials/adminHeader'); -%>
<div class="container">
    <div class="container">
      <br>
      <a class="waves-effect waves-light btn" href="/admin/leaderboardDownload">Download excel sheet</a>
      <a class="waves-effect waves-light btn" href="/admin/finalLeadDownload">Download bonus round excel sheet</a>
      <a class="waves-effect waves-light btn" href="/admin/setTopWinners">Set score and time to 0 for top 50</a>
      <table class="highlight centered ">
        <thead>
          <tr class="amber-text text-darken-3">
            <th>Rank</th>
            <th>Name</th>
            <th>Email</th>
            <th>Ph Number</th>
            <th>TT User ID</th>
            <th>Score</th>
            <th>Time(sec)</th>
          </tr>
        </thead>
  
        <tbody>
          <%for(let i=0;i< users.length; i++ ){%>
            <tr>
              <td><%=i+1%></td>
              <td><%=users[i].name%></td>
              <td><%=users[i].email%></td>
              <td><%=users[i].phNumber%></td>
              <td><%=users[i].regno%></td>
              <td><%=users[i].score%></td>
              <td><%=users[i].timeTaken/10%></td>
            </tr>
          <%}%>
          
          
        </tbody>
      </table>
    </div>
  
  </div>